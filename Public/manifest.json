{
  "name": "Alpha",
  "description": "Multipurpose cryptocurrency wallet",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": ["tabs", "storage", "cookies"],
  "action": {
    "default_popup": "index.html"
  }
}